{
  "compilerOptions": {
    "outDir": "lib",
    "moduleResolution": "node",
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "allowJs": true,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": true, 
    "sourceMap": true
  },
  "include": [
    "src"
  ], "paths": {
    "*": ["node_modules/*", "src/types/*"]
  }
}
